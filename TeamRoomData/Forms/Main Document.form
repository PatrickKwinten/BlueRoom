  <code event="queryopen">
    <lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	
	If source.isnewdoc Then
		Exit Sub
	End If
	
	ver = Evaluate("@version")	
	doctype = source.document.getitemvalue("DocType")
	defaultDocType = source.document.getitemvalue("DefaultDocType3")
	
	If source.editmode = True And Cint(ver(0)) &lt; 172 And doctype(0) = defaultDocType(0) Then
		Messagebox R5msg, 16, R5title
		continue = False
		Exit Sub
	End If
	
	
End Sub
</lotusscript>
  </code>
  <code event="querymodechange">
    <lotusscript>Sub Querymodechange(Source As Notesuidocument, Continue As Variant)
	
	ver = Evaluate("@version")	
	
	doctype = source.document.getitemvalue("DocType")
	defaultDocType = source.document.getitemvalue("DefaultDocType3")
	
	If Cint(ver(0)) &lt; 172 And doctype(0) = defaultDocType(0) Then
		Messagebox R5msg, 16, R5title
		continue = False
		Exit Sub
	End If
End Sub</lotusscript>
