                      <code event="click">
                        <lotusscript>Sub Click(Source As Button)
	'NOTES CLIENT only
	Dim session As New notessession
	Dim uiwork As New notesuiworkspace
	Dim db As notesdatabase
	Dim view As notesview
	Dim note As notesdocument
	Dim doc As notesdocument
	Dim uidoc As notesuidocument
	
	Set db =  session.currentdatabase
	Set view = db.getview("PeopleLookup")
	Set dbprofile = db.getprofiledocument("tempvars")
	Set doc = view.getfirstdocument
	Set coll = view.allentries
	Set uidoc = uiwork.currentdocument
	Set note = uidoc.document
	
	count = coll.count
	
	altnamedisplay = note.getitemvalue("AltNameDisplay")
	
	Redim Partic(count-1) As Variant
	count = 0
	
'get names of all active participants in the TeamRoom	
	While Not doc Is Nothing
		Call getNameValue(doc, altnamedisplay)
		Dim whoname As New notesname(whovalue(0))
		CNwho = whoname.abbreviated
		partic(count) = CNwho
		Set doc = view.getnextdocument(doc)
		count= count+1
	Wend
	
	If altnamedisplay(0) = "1" Then
		Call note.replaceitemvalue("AltNameSelection", partic)
	Else		
		Call note.replaceitemvalue("PriNameSelection", partic)
	End If
	
	Call uidoc.refresh
	
End Sub
</lotusscript>
                      </code>
                      <code event="getNameValue">
                        <lotusscript>Function getNameValue(doc As notesdocument, altnamedisplay As Variant)
	
'alternate name support	
	If altnamedisplay(0) = "1" Then
		whovalue = doc.getitemvalue("who")
	Else
		whovalue = doc.getitemvalue("who")
		
		'next if-section is for backwards compatibility with 5.0 teamroom, which does not have this field in the participant docs
		If whovalue(0) = "" Then
			whovalue = doc.getitemvalue("who")
		End If
	End If
	
	
End Function</lotusscript>
