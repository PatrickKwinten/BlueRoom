      <code event="click">
        <lotusscript>Sub Click(Source As Button)
	Dim db As Notesdatabase 
	Dim session As New NotesSession
	Dim workspace As New notesuiworkspace
	Dim uidoc As notesuidocument
	Dim note As notesdocument
	Dim item As NotesItem
	Dim tdrange As NotesItem
	Dim startdttm As NotesDateTime
	Dim enddttm As NotesDateTime
	Dim window As NotesDateRange
	Dim names As String
	Dim dur As Integer
	Dim username As String
	Dim mtgLoc As Variant
	Dim entryType As String
	Dim Invitees As Variant
	Dim origFrom As Variant
	
	Set uidoc =  workspace.currentdocument
	If uidoc.editmode Then
		uidoc.refresh		
	End If
	
	Set note = uidoc.Document
	Set currentdb = session.currentdatabase
	Set pnote = currentdb.getprofiledocument("tempvars")
	getlocalTime = pnote.localtime
	getendtime = pnote.endtime
	
	mtgDt = note.getitemvalue("MtgDate")
	
	mtgT = uidoc.fieldgettext("MtgTime")
	mtgU = uidoc.fieldgettext("Duration")
	
	Dim localDateTime As New notesdatetime(getLocalTime(0))	
	Dim localEndTime As New notesdatetime(getendtime(0))
	
	
	If mtgDt(0)  = "" Or mtgT = "" Or mtgU = "" Then
		Msgbox Msg
		Exit Sub
	End If
	
	
	Set db = New notesdatabase("", "")
	db.openmail
	
	Dim cnote As New NotesDocument(db)
	Dim rtitem As NotesRichTextItem
	
	mtgLoc = note.getitemvalue("MtgLocation")
	invitees = note.getitemvalue("People")
	origFrom = note.getitemvalue("From")
	TRName = note.getitemvalue("TeamRoomName")
	subj = note.getitemvalue("Subject")
	
	username = session.UserName
	cnote.From = note.From(0)
	
	If TRName(0) = "" Then
		MakeName = "TeamRoom"
	Else
		MakeName = TRName(0)
	End If
	cnote.Subject = MakeName + ": " + subj(0)
	
	Set rtitem = note.GetFirstItem("Body")
	Call rtitem.CopyItemToDocument(cnote, "Body")
	cnote.Form = "Appointment"  'dnt
	cnote.Principal = username
	cnote.Chair = username
	cnote.DocAuthors = username
	cnote.AppointmentType = "4"
	cnote.tmpWasMailed = "1"
	cnote.ExcludeFromView = "D"
	cnote.OrgTable = "C0"
	cnote.Alarms = "1"	
	cnote.Duration = note.Duration
	
	cnoteUNID = cnote.universalid
	Call cnote.ReplaceItemValue("APPTUNID", cnoteUNID)
	
	Call cnote.ReplaceItemValue("$BusyName", username)
	Call cnote.replaceitemvalue("Location", mtgLoc)
	Call cnote.replaceitemvalue("RequiredAttendees", Invitees)
	Call cnote.replaceitemvalue("tmpOwnerHW", origFrom)
	Call cnote.ReplaceItemValue("StartDateTime", localDateTime)	
	Call cnote.ReplaceItemValue("EndDateTime", localEndTime)	
	Call cnote.ReplaceItemValue("ReminderTime", localDateTime)
	Call cnote.ReplaceItemValue("CalendarDateTime", localDateTime)
	
	Set tdrange = note.GetFirstItem("TimeRange")
	Call cnote.ReplaceItemValue("TimeRange", tdrange)
	Call cnote.ReplaceItemValue("_ViewIcon", 10)
	Call cnote.ReplaceItemValue("$Alarm", 1)
	Call cnote.ReplaceItemValue("$AlarmOffset", -5)
	
	Call cnote.computewithform(0,0)	
	cnote.save True, True
	
	Messagebox DocAddedMsg, 0, Title2    
	
End Sub</lotusscript>
