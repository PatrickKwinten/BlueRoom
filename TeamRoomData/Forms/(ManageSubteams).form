@SetProfileField("TempVars"; "Notify"; "");

@If(@IsError(Members); @Return("");
AltNameDisplay = "1"; @SetField("MemberNames"; ST + @Name([Abbreviate]; ImpMembersA));

 @SetField("MemberNames"; ST + ImpMembers))</formula>
