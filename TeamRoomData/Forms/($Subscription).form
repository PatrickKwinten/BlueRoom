                        <pardef id="43" leftmargin="0.3000in" spaceafter="1.5" keepwithnext="true" keeptogether="true"/>
                        <par def="43">
                          <run>
                            <font size="9pt"/>
                            <field usenotesstyle="false" height="1.2500in" width="3.2500in" multiline="true" borderstyle="single" type="text" kind="editable" name="AdvFormula" description="Advanced formula" htmltitle="Advanced formula">
                              <code event="defaultvalue">
                                <formula>"SELECT @All"</formula>
                              </code>
                              <code event="inputtranslation">
                                <formula>@If( $QueryType = "all"; @Return( "Select @All"); $QueryType = "adv"; @Return( AdvFormula); Continue);

tAuthStr :="\"" + @Implode( @Name([Canonicalize]; ProfileAuthors); "\":\"") + "\"";
tAuth := @If( AuthorMatch = "1"; "@contains(@lowercase(from); " + @LowerCase(tAuthStr) + ") | @contains(@lowercase(AltFrom); " + @LowerCase(tAuthStr) + ") "; "");


tSubjStr := "\"" + @Implode( @LowerCase(ProfileStrings); "\":\"") + "\"";
tSubj := @If( SubjectMatch = "1";
	@If( SubjectOp = "!cont"; "!" ; "") +
	@If(
		SubjectOp = "cont" : "!cont" : "beg";
			@If(
				SubjectOp = "cont" : "!cont";
					"@Contains(";
					"@Begins(")
			+ "@LowerCase(Subject); " + tSubjStr + ")";
		"@Contains(@LowerCase(Subject); \"" + @Implode(@LowerCase(ProfileStrings); "\") &amp; @Contains(@LowerCase(Subject); \"" ) + "\")" );
"");

tCatStr := "\"" + @Implode( @LowerCase(ProfileCategories); "\":\"") + "\"";
tCat := @If(CategoryMatch = "1";  "@Contains(@LowerCase(Categories); " +tCatStr + ")"; "");

tSubtStr := "\"" + @Implode( @LowerCase(ProfileSubteams); "\":\"") + "\"";
tSubt := @If(SubteamMatch = "1";  "@Contains(@LowerCase(SubteamInterest); " +tSubtStr + ")"; "");


tEvStr := "\"" + @Implode( @LowerCase(ProfileEvents); "\":\"") + "\"";
tEv := @If(EventMatch = "1";  "@Contains(@LowerCase(Milestones); " +tEvStr + ")"; "");

tThrMStr := "\"" + @Implode( @LowerCase(ProfileThreadSubjects); "\":\"") + "\"";
tThrM := @If(ThreadMatch = "1";  "@LowerCase(Subject) = " +tThrMStr; "");

tThrRStr := "\"" + @Implode( @LowerCase(ProfileThreadSubjects); "\":\"") + "\"";
tThrR := @If(ThreadMatch = "1";  "@LowerCase(OriginalSubject) = " +tThrRStr ; "");

tQuery := @Implode( @Trim( tAuth : tSubj : tCat : tSubt : tEv : tThrM : tThrR);
@If( AndOr = "and" ; " &amp; "; " | "));
"SELECT " + @If( @Length( tQuery) = 0; "@All"; tQuery)</formula>
                              </code>
                            </field>
                          </run>
                          <button width="2in" widthtype="maximum" maxlines="1" wraptext="true" bgcolor="#d5ccbb">
                            <font size="9pt"/>Check Syntax<code event="options">
