      <font size="9pt"/>
    </columnheader>
    <code event="value">
      <formula>@If(form="Status";"Status";@If(form="SubteamStatus";"Status";@If(form="IndStat";"Status";form)))</formula>
    </code>
  </column>
  <column hidedetailrows="false" itemname="DocType" width="10" resizable="true" separatemultiplevalues="false" sortnoaccent="false" sortnocase="true" showaslinks="false">
