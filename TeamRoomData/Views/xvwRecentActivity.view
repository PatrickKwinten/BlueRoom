    </columnheader>
    <code event="value">
      <formula>@If(form = "MainTopic" | @IsResponseDoc; DocType; @If(form = "announcement"; "Announcement"; @If(form="Event"; "Event"; @If(form="Status"; "Status"; @If(form="Meeting";"Meeting"; @If(form="SubteamStatus";"Status";@If(form="IndStat";"Status"; "")))))))</formula>
    </code>
  </column>
  <column hidedetailrows="false" itemname="From" width="10" resizable="true" separatemultiplevalues="false" sortnoaccent="false" sortnocase="true" showaslinks="false">
