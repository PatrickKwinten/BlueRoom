										<xp:this.value><![CDATA[#{javascript:"/members.xsp?profile=" + @Name("[Abbreviate]", viewEntry.getColumnValue("From"))}]]></xp:this.value>
									</xp:link>
									<xp:label value="added comment" style="padding:0px 4px">
