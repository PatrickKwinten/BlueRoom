<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
  <managed-bean>
    <managed-bean-name>TeamController</managed-bean-name>
    <managed-bean-class>org.openntf.teamroom.TeamRoom</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>dao</managed-bean-name>
    <managed-bean-class>org.openntf.teamroom.DaoBean</managed-bean-class>
    <managed-bean-scope>view</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>CheckboxBean</managed-bean-name>
    <managed-bean-class>com.ibm.xsp.teamroom.beans.CheckboxBean</managed-bean-class>
    <managed-bean-scope>view</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>JSFUtil</managed-bean-name>
    <managed-bean-class>org.openntf.teamroom.JSFUtil</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean id="dBar">
    <managed-bean-name>DebugToolbarBean</managed-bean-name>
    <managed-bean-class>eu.linqed.debugtoolbar.DebugToolbar</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <!-- persistent logs to an external (OpenLog)/ current database -->
    <managed-property>
      <property-name>logDbPath</property-name>
      <value>OpenLog.nsf</value>
      <!-- can also be the full path to the log database, e.g. logs\openLog.nsf -->
    </managed-property>
    <managed-property>
      <property-name>logEnabled</property-name>
      <value>true</value>
    </managed-property>
    <managed-property>
      <property-name>logLevel</property-name>
      <value>warn</value>
    </managed-property>
  </managed-bean>
